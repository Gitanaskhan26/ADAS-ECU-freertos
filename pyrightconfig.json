# pyrightconfig.json - Python type checking configuration
{
  "include": [
    "scripts"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "build"
  ],
  "typeCheckingMode": "basic",
  "reportMissingImports": false,
  "reportMissingModuleSource": false,
  "pythonVersion": "3.10"
}
